{
	"scss": {
		"snippets": {

			"Dva": "@include debug-vertical-alignment();background-size: 100% \\$base-line-height;",
			"Cf": "@include clearfix();",
			"Alt": "@include adjust-leading-to(${1:1}${2:[, 14px]});",
			"Lh": "@include adjust-leading-to(${1:1}${2:[, 14px]});",
			"Eb": "@include establish-baseline($1px);",
			"Bru": "\\$base-rhythm-unit",
			"Vg": "@include vertical-groove(${1:\\$rhythm-offset}, ${2:\\$optional-spacing}, ${3:\\$optional-new-font-size}, ${4:\\$optional-from-font-size}, ${5:\\$optional-number-rhythm-units});",

			"Bb": "@include base-base(${1:\\$font-size}, ${2:\\$default-font-family}, ${3:\\$trailing-white-space}, ${4:\\$spacing});",


			"Br": "@include border-radius($1px);",
			"Fz": "@include adjust-font-size-to(${1:\\$font-size}, ${2:\\$rythm-units}, ${3:\\$current-size});",

			"Bxz": "@include box-sizing(${1:border-box});",
			"Bs": "@include box-shadow(${1:\\$color} ${2:\\$hoff} ${3:\\$voff} ${4:\\$blur} ${5:\\$spread} ${6:\\$inset});",
			"Ts": "@include text-shadow(${1:\\$hoff} ${2:\\$voff} ${3:\\$blur} ${4:\\$spread} ${5:\\$color});",
			"Sbs": "@include single-box-shadow(${1:\\$color}, ${2:\\$hoff}, ${3:\\$voff}, ${4:\\$blur}, ${5:\\$spread}, ${6:\\$inset});",
			"St": "@include single-transition(${1:\\$property}, ${2:200ms}, ${3:\\$ease-function}, ${4:\\$delay});",
			"Tr": "@include transition(${1:\\$property} ${2:200ms} ${3:\\ease-in-out} ${4:\\$delay});",
			"Sts":"@include single-text-shadow(${1:\\$hoff}, ${2:\\$voff}, ${3:\\$blur}, ${4:\\$spread}, ${5:\\$color})",
			"Op":"@include opacity(${1:.5});",
			"Yep":"@include yep(${1:rgba}) {$2};",
			"Nope":"@include nope(${1:rgba}) {$2};",
			"Om":"@include omega;$1",
			"Bg":"@include background($1);",
			"Lg":"linear-gradient(${1:\\$top} ${2:\\$left}, ${3:\\$color1}, ${4:\\$color2})",
			"Rg":"radial-gradient(${1:\\$color1}, ${2:\\$color2} ${3:\\$size})",

			"Mt": "@include margin-leader(${1:1}, ${2:\\$current-font-size});",
			"Mb": "@include margin-trailer(${1:1}, ${2:\\$current-font-size});",
			"Pt": "@include padding-leader(${1:1}, ${2:\\$current-font-size});",
			"Pb": "@include padding-trailer(${1:1}, ${2:\\$current-font-size});",

			"Sc": "@include span-columns(${1:\\$num-col}, ${2:\\$container-size});",
			"Pre": "@include pre(${1:\\$num-col}, ${2:\\$container-size});",
			"Post": "@include post(${1:\\$num-col}, ${2:\\$container-size});",
			"Nth": "$1:nth-child($2){\n$3\n}",
			"NthT": "$1:nth-of-type($2){\n$2\n}",
			"ab": "absolute;$1",
			"rel": "relative;$1",
			"inc": "@include $1;",
			"Bsp": "-ms-behavior: url(static/lib/background-size/backgroundsize.min.htc);$1",
			"Small": "///////////////\n///  SMALL  ///\n///////////////\n@include breakpoint(\"small\") {\n\t$1\n}\n///////////////\n///END SMALL///\n///////////////\n",
			"SmallOnly": "//////////////////\n//  SMALL ONLY  //\n//////////////////\n@include breakpoint(\"small_only\") {\n\t$1\n}\n//////////////////\n//END SMALL ONLY//\n//////////////////\n",
			"SmallJSOnly": "/////////////////////\n//  SMALL ONLY JS  //\n/////////////////////\n@include breakpoint(\"small_only--js\") {\n\t$1\n}\n/////////////////////\n//END SMALL ONLY JS//\n/////////////////////\n",
			"SmallJS": "////////////////\n//  SMALL JS  //\n////////////////\n@include breakpoint(\"small--js\") {\n\t$1\n}\n////////////////\n//END SMALL JS//\n////////////////\n",
			"Medium": "//////////////////\n//    MEDIUM    //\n//////////////////\n@include breakpoint(\"medium\") {\n\t$1\n}\n//////////////////\n//  END MEDIUM  //\n//////////////////\n",
			"MediumOnly": "//////////////////\n//MEDIUM ONLY//\n//////////////////\n@include breakpoint(\"medium_only\") {\n\t$1\n}\n//////////////////\n//END MEDIUM ONLY//\n//////////////////\n",
			"MediumJS": "/////////////////////\n//  MEDIUM JS  //\n/////////////////////\n@include breakpoint(\"medium--js\") {\n\t$1\n}\n/////////////////////\n//  END MEDIUM JS  //\n/////////////////////\n",
			"MediumJSOnly": "//////////////////////////\n//  MEDIUM ONLY JS  //\n//////////////////////////\n@include breakpoint(\"medium_only--js\") {\n\t$1\n}\n//////////////////////////\n//  END MEDIUM ONLY JS  //\n//////////////////////////\n",
			"Large": "//////////////////\n//    LARGE     //\n//////////////////\n@include breakpoint(\"large\") {\n\t$1\n}\n//////////////////\n//   END LARGE  //\n//////////////////\n",
			"LargeOnly": "//////////////////\n//LARGE ONLY//\n//////////////////\n@include breakpoint(\"large_only\") {\n\t$1\n}\n//////////////////\n//END LARGE ONLY//\n//////////////////\n",
			"LargeJS": "/////////////////////\n//   LARGE JS  //\n/////////////////////\n@include breakpoint(\"large--js\") {\n\t$1\n}\n/////////////////////\n//   END LARGE JS  //\n/////////////////////\n",
			"LargeJSOnly": "//////////////////////////\n//   LARGE ONLY JS  //\n//////////////////////////\n@include breakpoint(\"large_only--js\") {\n\t$1\n}\n//////////////////////////\n//  END LARGE ONLY JS   //\n//////////////////////////\n"
		}
	}
}
